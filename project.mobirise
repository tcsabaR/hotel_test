          "_customHTML": "<section class=\"form3\" group=\"Forms\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"bootstrap-datepicker, mbr-booking-datepicker\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Show Underline\" name=\"showUnderline\" condition=\"showSubtitle\">\n        <input type=\"color\" title=\"Underline Color\" name=\"underlineColor\" value=\"#ffffff\" condition=\"showUnderline && showSubtitle\">\n        <input type=\"checkbox\" title=\"Show First Item\" name=\"showFirstItem\" checked>\n        <input type=\"color\" title=\"1st Item BG Color\" name=\"firstBg\" value=\"#c19b76\" condition=\"showFirstItem\">\n        <input type=\"checkbox\" title=\"Show 1st Item Text\" name=\"showFirstText\" checked condition=\"showFirstItem\">\n        <input type=\"checkbox\" title=\"Show 1st Item Button\" name=\"showFirstButton\" checked condition=\"showFirstItem\">\n        <input type=\"checkbox\" title=\"Show Second Item\" name=\"showSecondItem\" checked>\n        <input type=\"color\" title=\"2nd Item BG Color\" name=\"secondBg\" value=\"#333333\" condition=\"showSecondItem\">\n        <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" checked condition=\"showSecondItem\">\n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#000000\" condition=\"showLine && showSecondItem\">\n        <input type=\"checkbox\" title=\"Show Items Title\" name=\"showItemsTitle\" checked condition=\"showFirstItem || showSecondItem\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"(showFirstItem || showSecondItem) && showForm\">\n        <input type=\"checkbox\" title=\"Show Form\" name=\"showForm\" checked>\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background6.jpg\" selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#efefef\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=2jqUN6d6ZyM\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n\n        <header condition=\"showForm\">Form Settings</header>\n        <input type=\"checkbox\" title=\"Show Labels\" name=\"showLabels\" checked condition=\"showForm\">\n        <input type=\"color\" title=\"Date Color\" name=\"dateColor\" value=\"#000000\" condition=\"showForm\">\n        <input type=\"text\" inline title=\"Min Date (mm/dd/yyyy)\" name=\"startDate\" value=\"10/01/2019\" condition=\"showForm\">\n        <input type=\"text\" inline title=\"Max Date (mm/dd/yyyy)\" name=\"endDate\" value=\"10/10/2020\" condition=\"showForm\">\n        <input type=\"color\" title=\"Datepicker BG Color\" name=\"calendarBg\" value=\"#ffffff\" condition=\"showForm\">\n        <input type=\"color\" title=\"Datepicker Text Color\" name=\"calendarText\" value=\"#000000\" condition=\"showForm\">\n        <input type=\"color\" title=\"Active Day Color\" name=\"activeColor\" value=\"#2b6477\" condition=\"showForm\">\n        <input type=\"color\" title=\"Active Day Text Color\" name=\"activeTextColor\" value=\"#ffffff\" condition=\"showForm\">\n        <input type=\"checkbox\" title=\"Show Lines\" name=\"showLines\" checked condition=\"showForm\">\n        <input type=\"color\" title=\"Lines Color\" name=\"linesColor\" value=\"#cccccc\" condition=\"showLines && showForm\">\n        <input type=\"color\" title=\"Form BG Color\" name=\"formBg\" value=\"#fcfcfc\" condition=\"showForm\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-lighter mbr-white align-center\" mbr-class=\"{'pb-3': !showUnderline}\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle, .underline\">\n            HOTEL LUXURY\n        </h3>\n        <div class=\"underline pb-3 align-center\" mbr-if=\"showUnderline && showSubtitle\">\n            <div class=\"line\"></div>\n        </div>\n        <h2 class=\"mbr-section-title main-title mbr-fonts-style mbr-white align-center\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".main-title\">\n            Check The Availability\n        </h2>\n        <div class=\"row justify-content-center\">\n            <div class=\"content-wrap col-12\" mbr-class=\"{'pt-5': showTitle || showSubtitle}\" mbr-if=\"showFirstItem || showSecondItem || showForm\">\n                <div class=\"first-item mbr-white\" mbr-class=\"{'col-lg-3': showSecondItem && showForm,\n                                 'col-lg-4': !showSecondItem || !showForm}\" mbr-if=\"showFirstItem\">\n                    <h3 class=\"mbr-section-title first-item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showItemsTitle\" data-app-selector=\".first-item-title\">EXTRA BOOKING OLDAL</h3>\n                    <p class=\"mbr-text item-text mbr-fonts-style mbr-lighter pt-3\" mbr-theme-style=\"display-7\" mbr-if=\"showFirstText\" data-app-selector=\".item-text, .item-btn\">Az Ön web oldalán extra booking oldal.\n                    </p>\n                    <div mbr-buttons class=\"mbr-section-btn item-btn pt-3\" mbr-theme-style=\"display-4\" mbr-if=\"showFirstButton\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-md btn-danger\" href=\"https://www.paypal.com/cgi-bin/webscr?cmd=_cart&amount=10000&item_name=Saj%C3%A1t+foglal%C3%A1s&currency_code=HUF\" data-app-placeholder=\"Type Text\" target=\"_blank\">MEGRENDELEM</a></div>\n                </div>\n                \n                <div class=\"second-item mbr-white\" mbr-class=\"{'col-lg-3': showFirstItem && showForm,\n                                 'col-lg-4': !showFirstItem || !showForm}\" mbr-if=\"showSecondItem\">\n                    <h3 class=\"mbr-section-title second-item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showItemsTitle\" data-app-selector=\".second-item-title\">Ön HOTEL -e</h3>\n                    <div class=\"info-column\" mbr-class=\"{'pt-3': showItemsTitle}\">\n                        <div mbr-text class=\"mbr-text check-in check-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".check-text\">\n                            <span>Érkez-és</span>\n                            <span>2:00 de - 7:00 de</span>\n                        </div>\n                        <div mbr-text class=\"mbr-text check-out check-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".check-text\">\n                            <span>Távo-zás</span>\n                            <span>8:00 de - 12:00 de</span>\n                        </div>\n                    </div>\n                </div>\n\n                <div mbr-form class=\"booking-datepicker\" mbr-class=\"{'col-lg-6': showFirstItem && showSecondItem,\n             